pg:MonoGroup
	a rdfs:Class ;
	rdfs:subClassOf pg:DiscreteGroup ;
	rdfs:label "Mono" ;
	pg:element [
		lv2:index 0 ;
		lv2:designation pg:center ;
	] .

pg:StereoGroup
	a rdfs:Class ;
	rdfs:subClassOf pg:DiscreteGroup ;
	rdfs:label "Stereo" ;
	pg:element [
		lv2:index 0 ;
